algoritmo "ContagemIntervalo"

var
    ini, fim, inc, i: inteiro

inicio
    escreva("Primeiro valor: ")
    leia(ini)
    escreva("Último valor: ")
    leia(fim)
    escreva("Incremento: ")
    leia(inc)
    escreva("Contagem: ")
    i <- ini
    enquanto i <= fim faca
        escreva(i, " ")
        i <- i + inc
    fimenquanto
    escreval("Fim!")

fimalgoritmo